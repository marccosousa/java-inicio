import java.util.Scanner;

public class ex26_nelio {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		double soma; 
		int N = sc.nextInt();
		double vet[] = new double[N];
		
		for(int i=0; i<vet.length; i++) {
			vet[i] = sc.nextDouble();
			soma += vet[i];
		}
		
		sc.close();

	}

}
